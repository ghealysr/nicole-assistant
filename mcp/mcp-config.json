{
  "servers": {
    "brave-search": {
      "image": "mcp/brave-search:latest",
      "enabled": true,
      "env": {
        "BRAVE_API_KEY": "${BRAVE_API_KEY}"
      }
    },
    "filesystem": {
      "image": "mcp/filesystem:latest",
      "enabled": true,
      "mounts": {
        "/data": "/opt/nicole/data:ro"
      }
    }
  }
}

